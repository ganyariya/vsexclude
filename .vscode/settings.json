{
	"deno.enable": true,
	"deno.lint": true,
	"deno.unstable": true,
	"deno.suggest.imports.hosts": {
		"https://deno.land": false
	},
	"files.exclude": {
		"**/*.mypy_cache": true,
		"**/*venv": true,
		"**/*develo-eggs": true,
		"**/*eggs": true,
		"**/*wheels": true,
		"**/.pytype": true,
		"**/*.egg-info": true,
		"**/*.egg": true,
		"**/sdist": true,
		"**/*.manifest": true,
		"**/*.spec": true,
		"**/*.cache": true,
		"**/*.ipynb_checkpoints": true,
		"**/profile_default": true,
		"**/ipython_config.py": true,
		"**/__pypackages__": true,
		"**/*dmypy.json": true
	}
}